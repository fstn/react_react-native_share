  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build-css": "npx node-sass-chokidar src/ -o src/",
    "remove-css-random": "cp $(ls build/static/css/*.css | awk '{print $1}') build/static/css/main.css",
    "watch-css": "npx npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "watch": "onchange 'src/**/*.js' -- npm run build"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "16.3.1"
  },
  "devDependencies": {
