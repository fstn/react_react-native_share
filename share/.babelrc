{
  "env":{
    "development": {
      "plugins": [
        "react-native-classname-to-style",
        [
          "react-native-platform-specific-extensions",
          {
            "extensions": [
              "scss",
              "sass"
            ]
          }
        ],
        "transform-react-jsx-source"
      ]
    }
  },
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    "external-helpers"
  ]

}
